{
  "name": "volleyball-rating",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently -n NEXT,INIT_LONGPOLLING -c blue,green \"next dev --turbopack\" \"wait-on http://localhost:3000 && curl -s -o /dev/null http://localhost:3000/api/bot/longpolling\"",
    "build": "next build",
    "start": "next start",
